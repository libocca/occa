kernel void addVectors(const int entries,
                       const float *a,
                       const float *b,
                       float *ab){
  // int i;
  // for(i = 0; i < entries; ++i; tile(16)){
  for(int2 i = {0,0}; i.y < entries, i.x < entries; ++i.y, ++i.x; tile(16,16)){
    if(i < entries)
      ab[i] = a[i] + b[i];
  }
}
